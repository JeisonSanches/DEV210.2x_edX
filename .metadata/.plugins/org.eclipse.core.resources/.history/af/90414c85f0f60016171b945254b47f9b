#ifndef DONATION_H_
#define DONATION_H_

#pragma once
#include <sstream>

class Donation {
public:
	Donation();
	virtual ~Donation();
};

std::ostream & operator <<(std::ostream & os, const Donation & ts);
std::istream & operator >>(std::istream & os, const Donation & ts);

#endif /* DONATION_H_ */
