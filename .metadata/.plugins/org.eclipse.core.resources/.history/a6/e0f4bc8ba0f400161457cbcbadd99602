#ifndef STUDENT_H_
#define STUDENT_H_

#include "Person.h"

class Student : public Person {
public:
	Student();
	Student(std::string, std::string, int, std::string, long);
	virtual ~Student();

	virtual void OutputIdentity() = 0;
	virtual void OutputAge();
};

#endif /* STUDENT_H_ */
