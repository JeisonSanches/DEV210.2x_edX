#include "demoMiscManipulators.h"

void demoMiscManipulators() {

	int x = 3;
	int y = 4;
	int z = -5;

	// Showing/hiding + sign
	std::cout << x << "" << y << " " << z << std::endl;
	std::cout << std::showpos;
	std::cout << x << "" << y << " " << z << std::endl;
	std::cout << std::noshowpos;
	std::cout << x << "" << y << " " << z << std::endl << std:: endl;


}


