#ifndef TEMPSTAT_H_
#define TEMPSTAT_H_

#pragma once
#include <iostream>

class TempStat {
public:
	double minimum, maximum;

	TempStat(double minimum = 0.0, double maximum = 0.0)
		: minimum(minimum), maximum(maximum) {}

	virtual ~TempStat();
};

#endif /* TEMPSTAT_H_ */
