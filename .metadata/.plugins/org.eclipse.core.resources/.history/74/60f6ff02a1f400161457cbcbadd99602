#ifndef TEACHER_H_
#define TEACHER_H_

class Teacher : public Person {
public:
	Teacher();
	Teacher(std::string, std::string, int, std::string, long);
	virtual ~Teacher();

	virtual void OutputAge();
	// Pure virtual
	virtual void OutputIdentity();
};

#endif /* TEACHER_H_ */
