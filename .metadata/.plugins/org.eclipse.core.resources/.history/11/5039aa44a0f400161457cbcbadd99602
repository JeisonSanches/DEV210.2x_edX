#pragma once
#include "Teacher.h"
#include "Person.h"

Teacher::Teacher() {}

Teacher::~Teacher() {}

virtual void Person::OutputIdentity() {
	std::cout << "I am a teacher" << std::endl;
}

void Student::OutputAge() {
	OutputIdentity();
	Person::OutputAge();
}

